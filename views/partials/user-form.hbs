<div class="mb-3">
    <label for="floatingInput" class="form-label">First Name</label>
    <input type="text" class="form-control" id="floatingInput" name="first_name" value="{{this.first_name}}" placeholder="First Name" aria-describedby="firstName" placeholder="First Name">
</div>
<div class="mb-3">
    <label for="floatingInput" class="form-label">Last Name</label>
    <input type="text" class="form-control" id="floatingInput" name="last_name" value="{{this.last_name}}" aria-describedby="firstName" placeholder="Last Name">
</div>
<div class="mb-3">
    <label for="floatingInput" class="form-label">Email</label>
    <input type="email" class="form-control" id="floatingInput" name="email" value="{{this.email}}" aria-describedby="email" placeholder="email@email.com">
</div>
<div class="mb-3">
    <label for="floatingInput" class="form-label">Phone</label>
    <input type="text" class="form-control" id="floatingInput" name="phone" value="{{this.phone}}" aria-describedby="phone" placeholder="Phone">
</div>
<div class="mb-3">
    <label for="floatingInput" class="form-label">Comments</label>
    <textarea class="form-control" id="floatingInput" rows="3" name="comment" placeholder="Comments">{{this.comment}}</textarea>
</div>
<button type="submit" class="btn btn-primary">Submit</button>